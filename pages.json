{
	
	"easycom": {
			"autoscan": true,
			// 注意一定要放在custom里，否则无效，https://ask.dcloud.net.cn/question/131175
			"custom": {
				"^u--(.*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue",
				"^up-(.*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue",
		    "^u-([^-].*)": "@/uni_modules/uview-plus/components/u-$1/u-$1.vue"
			}
		},
	
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		},
		{
			"path" : "pages/index/toolbar/version",
			"style" : 
			{
				"navigationBarTitleText" : ""
			}
		},
		{
			"path" : "pages/index/toolbar/cameria",
			"style" : 
			{
				"navigationBarTitleText" : ""
			}
		},
		{
			"path" : "pages/index/toolbar/history",
			"style" : 
			{
				"navigationBarTitleText" : ""
			}
		},
		{
			"path" : "pages/index/toolbar/vedio",
			"style" : 
			{
				"navigationBarTitleText" : ""
			}
		}
	],
	"tabBar": {
			"custom":true,
			"list": [
				{
					"pagePath": "pages/index/toolbar/cameria"
				},
				{
					"pagePath": "pages/index/toolbar/vedio"
				},
				{
					"pagePath": "pages/index/toolbar/history"
				},
				{
					"pagePath":  "pages/index/toolbar/version"
				} 
			]
		},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}
